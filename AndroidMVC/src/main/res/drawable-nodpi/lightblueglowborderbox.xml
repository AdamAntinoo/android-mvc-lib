<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:bottom="1px"
        android:left="1px"
        android:right="1px"
        android:top="1px">
        <shape android:shape="rectangle">

            <padding
                android:bottom="1dip"
                android:left="1dip"
                android:right="1dip"
                android:top="1dip" />

            <solid android:color="@android:color/transparent" />

            <stroke
                android:width="2px"
                android:color="#6033CCFF" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">

            <padding
                android:bottom="1dip"
                android:left="1dip"
                android:right="1dip"
                android:top="1dip" />

            <solid android:color="@android:color/transparent" />

            <stroke
                android:width="1px"
                android:color="@color/appbluelight" />

        </shape>
    </item>
</layer-list>



