<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
	<item>
		<shape android:shape="rectangle">
			<solid android:color="@color/uipanelborder" />
		</shape>
	</item>
	<item
		android:bottom="@dimen/panelborderwidth"
		android:left="@dimen/panelborderwidth"
		android:right="@dimen/panelborderwidth"
		android:top="@dimen/panelborderwidth">
		<shape android:shape="rectangle">
			<solid android:color="@color/uipaneldataareabackground" />
		</shape>
	</item>
	<item
		android:left="0dp"
		android:top="0dp"
		android:width="20dp"
		android:height="20dp">
		<vector xmlns:android="http://schemas.android.com/apk/res/android"
			android:width="40dp"
			android:height="40dp"
			android:viewportWidth="100"
			android:viewportHeight="100">
			<group android:name="triableGroup">
				<path
					android:name="triangle"
					android:fillColor="@color/appwhite"
					android:pathData="M0,0 l100,0 -100,100 Z" />
			</group>
		</vector>
	</item>
	<item
		android:left="0dp"
		android:top="0dp"
		android:width="20dp"
		android:height="20dp">
		<vector xmlns:android="http://schemas.android.com/apk/res/android"
			android:width="40dp"
			android:height="40dp"
			android:viewportWidth="100"
			android:viewportHeight="100">
			<group android:name="triableGroup">
				<path
					android:name="triangle"
					android:fillColor="@color/appred"
					android:pathData="M5,5 l85,0 -85,85 Z" />
			</group>
		</vector>
	</item>
	<item
		android:left="0dp"
		android:top="0dp"
		android:width="20dp"
		android:height="20dp">
		<vector xmlns:android="http://schemas.android.com/apk/res/android"
			android:width="40dp"
			android:height="40dp"
			android:viewportWidth="100"
			android:viewportHeight="100">
			<group android:name="triableGroup">
				<path
					android:name="triangle"
					android:fillColor="@color/appwhite"
					android:pathData="M5,5 l85,0 -85,85 Z" />
			</group>
		</vector>
	</item>
</layer-list>
